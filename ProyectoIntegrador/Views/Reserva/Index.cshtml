@model IEnumerable<ProyectoIntegrador.Models.Reserva>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CostoReserva)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaReserva)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraReserva)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdInstalacionDeportivaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUperfilNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CostoReserva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaReserva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraReserva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdInstalacionDeportivaNavigation.IdInstalacionDeportiva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUperfilNavigation.IdUperfil)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdReserva">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdReserva">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdReserva">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
